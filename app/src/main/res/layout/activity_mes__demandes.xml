<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/large_margin12"
    android:layout_marginRight="@dimen/large_margin12"
    android:layout_marginTop="@dimen/large_margin12"
    tools:context="com.example.eleves.android_comptoir_grasset.Mes_Demandes">



    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayoutTop"
        >

        <TextView
            android:text="MES DEMANDES"
            android:textSize="@dimen/tx_size20"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />





        <ListView
            android:layout_marginTop="@dimen/margin35"
            android:id="@+id/list_demandes"
            android:layout_width="match_parent"
            android:layout_height="335dp"></ListView>



    </LinearLayout>


</RelativeLayout>
